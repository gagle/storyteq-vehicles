{
  "name": "vehicle-state",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/vehicle/state/src",
  "prefix": "st",
  "projectType": "library",
  "tags": ["domain:vehicle", "type:state", "vehicle-state"],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/vehicle/state/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "cache": true,
        "cacheLocation": "eslint_cache",
        "outputs": ["{options.outputFile}", "{workspaceRoot}/eslint_cache/vehicle-state"]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc -p libs/vehicle/state/tsconfig.spec.json --noEmit"]
      }
    }
  }
}

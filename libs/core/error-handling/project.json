{
  "name": "core-error-handling",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/error-handling/src",
  "prefix": "st",
  "projectType": "library",
  "tags": ["scope:core", "type:api"],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/core/error-handling/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "cache": true,
        "cacheLocation": "eslint_cache",
        "outputs": ["{options.outputFile}", "{workspaceRoot}/eslint_cache/core-error-handling"]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc -p libs/core/error-handling/tsconfig.spec.json --noEmit"]
      }
    }
  }
}
